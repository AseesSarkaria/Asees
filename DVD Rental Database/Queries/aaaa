CREATE OR REPLACE FUNCTION infer_symbol_by_column(value NUMERIC, column_name TEXT)
RETURNS TEXT AS $$
BEGIN
    RETURN CASE 
        WHEN column_name LIKE '%_p' THEN CONCAT(value, '%')
        WHEN column_name LIKE '%_m' THEN CONCAT('$', value)
        -- Add additional cases as needed, e.g., for different suffixes
        ELSE value::TEXT
    END;
END;
$$ LANGUAGE plpgsql;
